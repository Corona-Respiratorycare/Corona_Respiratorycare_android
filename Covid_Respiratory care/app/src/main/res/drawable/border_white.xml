<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:left="-10dp"
        >

        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" />
            <corners android:radius="10dp" />
        </shape>
    </item>

    <item
        android:top="-10dp"
        android:right="-10dp"
        android:left="-20dp"
        >
        <shape android:shape="rectangle">
            <stroke
                android:width="3dp"
                android:color="#165188"/>
            <corners
                android:radius="20dp"/>
        </shape>
    </item>
</layer-list>